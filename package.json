{
  "name": "react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-scripts": "1.1.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom --coverage --coverageDirectory=output/coverage/react",
    "eject": "react-scripts eject",
    "lint": "eslint src/**/*.js"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/Blocks/**/*.js",
      "!<rootDir>/build/",
      "!<rootDir>/node_modules/",
      "!<rootDir>/output/",
      "!<rootDir>/public/"
    ]
  },
  "devDependencies": {
    "eslint": "^5.6.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0"
  }
}
